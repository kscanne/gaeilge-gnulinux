msgid "TrueType fonts manager. Still in early stage.\n"
msgstr ""

#: drakfont.c:1736
msgid "  -p, --pos                  launch at specified position x, y\n"
msgstr ""

#: drakfont.c:1737
msgid "  -c, --check                check fonts locations\n"
msgstr ""

#: drakfont.c:1738
msgid ""
"  -r, --rescue               rescue mode: rebuild font description files "
