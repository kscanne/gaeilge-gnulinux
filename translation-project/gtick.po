#: src/gtick.c:81
#, c-format
msgid ""
"Usage: %s [OPTION...]\n"
"\n"
"Options:\n"
"  -h, --help              Show this help message\n"
"  -v, --version           Print version information\n"
"  -d, --debug[=level]     Print additional runtime debugging data to stdout\n"
"\n"
msgstr ""
"Úsáid: %s [ROGHA...]\n"
"\n"
"Roghanna:\n"
"  -h, --help              Taispeáin an chabhair seo\n"
"  -v, --version           Taispeáin eolas faoin leagan\n"
"  -d, --debug[=leibhéal]  Taispeáin tuilleadh eolais dífhabhtaithe\n"
"\n"

